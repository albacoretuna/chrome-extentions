{
    "name": "Frosmo Inspector",
    "version": "0.1",
    "manifest_version": 2,
    "content_scripts": [{
        "js": ["contentscript.js"],
        "run_at": "document_end",
        "matches": ["<all_urls>"]
    }],
    "web_accessible_resources": ["script.js"],
    "browser_action": {
         "default_icon": "icon.png",
         "default_popup": "popup.html",
         "default_title": "Frosmo Inspector"
    }
}
